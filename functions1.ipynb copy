   "source": [
    "#Poisson Distribution converging to the Normal Distribution\n",
    "\n",
    "%matplotlib inline\n",
    "from __future__ import division      \n",
    "import numpy as np\n",
    "import scipy.stats as stats     # for pdfs \n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N_1 = 100\n",
    "p_1 = 0.5\n",
    "n_1 = np.arange(0, 100)\n",
    "y_1 = stats.binom.pmf(n_1, N_1, p_1)\n",
    "\n",
    "#for i in [1, 5, 10, 20, 50, 100]:\n",
    "    \n",
    "\n",
    "plt.plot(n_1,y_1,'o-', color='#348ABD')\n",
    "plt.title('Binomial: N=%i , p=%.2f' % (N_1,p_1),fontsize=15)\n",
    "plt.xlabel('x',fontsize=15)\n",
    "plt.ylabel('Probability density',fontsize=15);"
   ]
