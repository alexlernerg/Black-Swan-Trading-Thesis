   "source": [
    "SPX = pd.read_csv('snp_15_real.csv', index_col='Date')\n",
    "SPX['Log Price'] = log(SPX['Adj Close'])\n",
    "SPX['Returns'] = SPX['Adj Close'].pct_change()\n",
    "SPX.index = pd.to_datetime(SPX.index)\n",
    "SPX_returns = SPX['Returns'][1:,]\n",
    "\n",
    "portfolio = pd.read_csv('portfolio.csv', index_col='Date')\n",
    "portfolio['Log Price'] = log(portfolio['Close'])\n",
    "portfolio['Returns'] = portfolio['Close'].pct_change()\n",
    "portfolio['Log Returns'] = portfolio['Log Price'].pct_change()\n",
    "portfolio.index = pd.to_datetime(portfolio.index)\n",
    "portfolio_returns = portfolio['Returns'][1:,]\n",
    "\n",
    "strategy = pd.DataFrame([SPX['Log Price']], SPX['Returns'], )\n",
    "\n",
    "# Create a Gometric Brownian Motion, Mean-Reverting and Trending Series\n",
    "gbm = log(cumsum(random.randn(100000))+1000)\n",
    "mr = log(random.randn(100000)+1000)\n",
    "tr = log(cumsum(random.randn(100000)+1)+1000)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 433,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from statsmodels.tsa.stattools import acf\n",
    "from statsmodels.graphics.tsaplots import plot_acf\n",
    "import matplotlib.pyplot as plt\n",
    "from math import sqrt\n",
    "import matplotlib.pyplot as plt  \n",
    "from numpy import * \n",
    "from statsmodels.tsa.stattools import adfuller\n",
    "\n",
    "#Defining what is the threshold of the Black Swan\n",
    "threshold = 0.03\n",
    "\n",
    "#Defining a new index with: -1 = Negative Black Swan, 1 = Positive Black Swan\n",
    "def decide(x):\n",
    "    if (x > threshold):\n",
    "        return 1\n",
    "    if (x < -threshold):\n",
    "        return -1\n",
    "    return 0\n",
    "SPX['decide'] = SPX['Returns'].apply(decide)\n",
    "\n",
    "#3 Tests for Mean Reversion/Stationarity\n",
    "\n",
    "def autocorr_daily(df, ticker):\n",
    "\n",
    "    \"test for daily autocorrelation\"\n",
    "    \n",
    "    returns = df.pct_change()\n",
    "    autocorrelation = returns['Log Price'].autocorr()\n",
    "    \n",
    "    if autocorrelation < 0:\n",
    "        print('Ticker {} is daily mean reverting with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    else:\n",
    "        print('Ticker {} is momentum bound or trending with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "\n",
    "def autocorr_weekly(df, ticker):\n",
    "    \"test for weekly autocorrelation\"\n",
    "    \n",
    "    ticker_weekly = df.resample(rule='W').last()\n",
    "    returns = ticker_weekly.pct_change()\n",
    "    autocorrelation = returns['Log Price'].autocorr()\n",
    "    \n",
    "    if autocorrelation < 0:\n",
    "        print('Ticker {} is weekly mean reverting with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    else:\n",
    "        print('Ticker {} is momentum bound or trending with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    \n",
    "    return\n",
    "\n",
    "def autocorr_monthly(df, ticker):\n",
    "    \"test for monthly autocorrelation\"\n",
    "    \n",
    "    ticker_monthly = df.resample(rule='M').last()\n",
    "    returns = ticker_monthly.pct_change()\n",
    "    autocorrelation = returns['Log Price'].autocorr()\n",
    "    \n",
    "    if autocorrelation < 0:\n",
    "        print('Ticker SPX is monthly mean reverting with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    else:\n",
    "        print('Ticker {} is montly momentum bound or trending with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    \n",
    "    return\n",
    "\n",
    "def autocorr_yearly(df, ticker):\n",
    "    \"test for monthly autocorrelation\"\n",
    "    \n",
    "    ticker_yearly = df.resample(rule='12M').last()\n",
    "    returns = ticker_yearly.pct_change()\n",
    "    autocorrelation = returns['Log Price'].autocorr()\n",
    "    \n",
    "    if autocorrelation < 0:\n",
    "        print('Ticker {} is yearly mean reverting with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    else:\n",
    "        print('Ticker {} is momentum bound or trending with autocorrelation: {}'.format(ticker, autocorrelation))\n",
    "    \n",
    "    return\n",
    "\n",
    "def autocorrelation_function(df):\n",
    "    autocorrelation = returns['Log Price'].autocorr()\n",
    "    #Plotting the ACF\n",
    "    #acf function, 95% CI w/ alpha=.05\n",
    "    plot_acf(returns, alpha=0.05, lags=365)\n",
    "    plt.xlabel('Lag (days)'); plt.title(\"Autocorrelation Function, 95% CI with alpha = 0.05\")\n",
    "    plt.show()\n",
    "\n",
    "def hurst_exp(df):\n",
    "    \"\"\"Returns the Hurst Exponent of the time series vector df\"\"\"\n",
    "    lags = range(2, 150)\n",
    "\n",
    "    # Calculate the array of the variances of the lagged differences\n",
    "    tau = [sqrt(std(subtract(df[lag:], df[:-lag]))) for lag in lags]\n",
    "\n",
    "    # Use a linear fit to estimate the Hurst Exponent\n",
    "    poly = polyfit(log(lags), log(tau), 1)\n",
    "\n",
    "    # Return the Hurst exponent from the polyfit output\n",
    "    return poly[0]*2.0\n",
    "    \n",
    "def variance_ratio(ts, lag = 2):\n",
    "    \"\"\"\n",
    "    Returns the variance ratio test result\n",
    "    \"\"\"\n",
    "    # make sure we are working with an array, convert if necessary\n",
    "    ts = np.asarray(ts)\n",
    "    \n",
    "    # Apply the formula to calculate the test\n",
    "    n = len(ts)\n",
    "    mu  = sum(ts[1:n]-ts[:n-1])/n;\n",
    "    m=(n-lag+1)*(1-lag/n);\n",
    "    b=sum(np.square(ts[1:n]-ts[:n-1]-mu))/(n-1)\n",
    "    t=sum(np.square(ts[lag:n]-ts[:n-lag]-lag*mu))/m\n",
    "    return t/(lag*b);"
   ]
  },
